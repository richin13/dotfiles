{
  "coc.preferences.extensionUpdateCheck": "daily",
  "suggest.autoTrigger": "always",
  "tsserver.npm": "/home/rmadriz/.asdf/shims/npm",
  "pyright.disableCompletion": true,
  "pyright.disableProgressNotifications": true,
  "python.formatting.provider": "black",
  "python.linting.pylintEnabled": true,
  "diagnostic.errorSign": "λ",
  "diagnostic.warningSign": "π",
  "diagnostic.hintSign": ">",
  "diagnostic.infoSign": "!",
  "diagnostic-languageserver.trace.server": "messages",
  "diagnostic-languageserver.filetypes": {
    "python": ["pylint", "mypy"],
    "nginx": ["nginx-linter"]
  },
  "diagnostic-languageserver.linters": {
    "nginx-linter": {
      "sourceName": "nginx-linter",
      "command": "nginx-linter",
      "debounce": 100,
      "args": ["--include", "%file"],
      "formatPattern": [
        "^\\s+(\\d+?):(\\d+?)\\s+([a-z]+?)\\s+(.*)$",
        {
          "line": 1,
          "column": 2,
          "security": 3,
          "message": 4
        }
      ],
      "rootPatterns": ["nginx.conf", ".git"],
      "securities": {
        "error": "info"
      }
    }
  },
  "highlight.colorNames.enable": false,
  "yaml.schemas": {
    "https://raw.githubusercontent.com/OAI/OpenAPI-Specification/3.0.3/schemas/v3.0/schema.json": [
      "/*open-api*.yml",
      "/*open-api*.yaml"
    ],
    "https://raw.githubusercontent.com/softprops/github-actions-schemas/master/workflow.json": [
      ".github/workflows/*.yml"
    ]
  },
  "languageserver": {
    "haskell": {
      "command": "haskell-language-server-wrapper",
      "args": ["--lsp"],
      "rootPatterns": [
        "*.cabal",
        "stack.yaml",
        "cabal.project",
        "package.yaml",
        "hie.yaml"
      ],
      "filetypes": ["haskell", "lhaskell"]
    }
  },
  "svelte.enable-ts-plugin": true
}
