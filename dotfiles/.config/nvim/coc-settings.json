{
  "coc.preferences.extensionUpdateCheck": "never",
  "suggest.autoTrigger": "always",
  "tsserver.npm": "/home/rmadriz/.asdf/shims/npm",
  "diagnostic.errorSign": "λ",
  "diagnostic.warningSign": "π",
  "diagnostic-languageserver.trace.server": "messages",
  "diagnostic-languageserver.filetypes": {
    "python": ["pylint", "mypy"],
    "sh": ["shellcheck"],
    "nginx": ["nginx-linter"]
  },
  "diagnostic-languageserver.linters": {
    "shellcheck": {
      "command": "shellcheck",
      "debounce": 100,
      "args": ["--format", "json", "-"],
      "sourceName": "shellcheck",
      "parseJson": {
        "line": "line",
        "column": "column",
        "endLine": "endLine",
        "endColumn": "endColumn",
        "message": "${message} [${code}]",
        "security": "level"
      },
      "securities": {
        "error": "error",
        "warning": "warning",
        "info": "info",
        "style": "hint"
      }
    },
    "nginx-linter": {
      "sourceName": "nginx-linter",
      "command": "nginx-linter",
      "debounce": 100,
      "args": ["--include", "%file"],
      "formatPattern": [
        "^\\s+(\\d+?):(\\d+?)\\s+([a-z]+?)\\s+(.*)$",
        {
          "line": 1,
          "column": 2,
          "security": 3,
          "message": 4
        }
      ],
      "rootPatterns": ["nginx.conf", ".git"],
      "securities": {
        "error": "info"
      }
    }
  },
  "highlight.colorNames.enable": false,
  "yaml.schemas": {
    "https://raw.githubusercontent.com/OAI/OpenAPI-Specification/3.0.3/schemas/v3.0/schema.json": [
      "/*open-api*.yml",
      "/*open-api*.yaml"
    ]
  },
  "languageserver": {
    "haskell": {
      "command": "haskell-language-server-wrapper",
      "args": ["--lsp"],
      "rootPatterns": [
        "*.cabal",
        "stack.yaml",
        "cabal.project",
        "package.yaml",
        "hie.yaml"
      ],
      "filetypes": ["haskell", "lhaskell"]
    }
  },
  "svelte.enable-ts-plugin": true
}
