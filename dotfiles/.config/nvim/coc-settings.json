{
  "suggest.autoTrigger": "always",
  "notification.disabledProgressSources": ["*"],
  "tsserver.npm": "/home/rmadriz/.asdf/shims/npm",
  "inlayHint.enable": false,
  "python.formatting.provider": "black",
  "python.linting.pylintEnabled": true,
  "diagnostic.errorSign": " ",
  "diagnostic.warningSign": " ",
  "diagnostic.hintSign": " ",
  "diagnostic.infoSign": " ",
  "diagnostic-languageserver.trace.server": "messages",
  "diagnostic-languageserver.filetypes": {
    "python": ["ruff", "mypy"],
    "nginx": ["nginx-linter"]
  },
  "diagnostic-languageserver.linters": {
    "nginx-linter": {
      "sourceName": "nginx-linter",
      "command": "nginx-linter",
      "debounce": 100,
      "args": ["--include", "%file"],
      "formatPattern": [
        "^\\s+(\\d+?):(\\d+?)\\s+([a-z]+?)\\s+(.*)$",
        {
          "line": 1,
          "column": 2,
          "security": 3,
          "message": 4
        }
      ],
      "rootPatterns": ["nginx.conf", ".git"],
      "securities": {
        "error": "info"
      }
    },
    "ruff": {
      "sourceName": "ruff",
      "command": "ruff",
      "args": ["--quiet", "%file"],
      "formatPattern": [
        "^[\\w/\\.]+:(\\d+):(\\d+):\\s([A-Z]{1,3}\\d+)\\s(.+)$",
        {
          "line": 1,
          "column": 2,
          "security": 3,
          "message": 4
        }
      ],
      "securities": {
        "F": "error",
        "E": "warning",
        "W": "warning",
        "I": "info",
        "D": "warning",
        "U": "info",
        "N": "warning",
        "S": "info",
        "C": "warning",
        "FBT": "warning",
        "B": "warning",
        "A": "info",
        "T": "hint",
        "Q": "hint",
        "ANN": "warning",
        "M": "hint"
      }
    }
  },
  "highlight.colorNames.enable": false,
  "yaml.schemas": {
    "https://raw.githubusercontent.com/OAI/OpenAPI-Specification/3.0.3/schemas/v3.0/schema.json": [
      "/*open-api*.yml",
      "/*open-api*.yaml"
    ],
    "https://raw.githubusercontent.com/softprops/github-actions-schemas/master/workflow.json": [
      ".github/workflows/*.yml"
    ]
  },
  "languageserver": {
    "terraform": {
      "command": "terraform-ls",
      "args": ["serve"],
      "filetypes": ["terraform", "tf"],
      "initializationOptions": {},
      "settings": {}
    }
  },

  "suggest.completionItemKindLabels": {
    "keyword": "\uf1de",
    "variable": "\ue79b",
    "value": "\uf89f",
    "operator": "\u03a8",
    "function": "\u0192",
    "reference": "\ufa46",
    "constant": "\uf8fe",
    "method": "\uf09a",
    "struct": "\ufb44",
    "class": "\uf0e8",
    "interface": "\uf417",
    "text": "\ue612",
    "enum": "\uf435",
    "enumMember": "\uf02b",
    "module": "\uf40d",
    "color": "\ue22b",
    "property": "\ue624",
    "field": "\uf9be",
    "unit": "\uf475",
    "event": "\ufacd",
    "file": "\uf723",
    "folder": "\uf114",
    "snippet": "\ue60b",
    "typeParameter": "\uf728",
    "default": "\uf29c"
  },
  "svelte.enable-ts-plugin": true,
  "hover.floatConfig": {
    "borderhighlight": "Boolean",
    "winblend": 10,
    "border": true
  }
}
